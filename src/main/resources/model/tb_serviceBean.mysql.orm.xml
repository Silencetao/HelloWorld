<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
    <package>com.newtouch.model</package>
    <access>PROPERTY</access>
    
    <entity class="com.newtouch.model.User">
    	<table name="tb_user" />
    	<attributes>
    		<id name="id">
    			<column name="ID" unique="true" nullable="false" precision="19" scale="0" />
    			<generated-value strategy="IDENTITY" />
    		</id>
    		<basic name="email">
    			<column name="email" length="20" nullable="true" />
    		</basic>
    		<basic name="password">
    			<column name="password" length="20" nullable="true" />
    		</basic>
    		<basic name="username">
    			<column name="username" length="20" nullable="true" />
    		</basic>
    		<basic name="sex">
    			<column name="sex" nullable="true" />
    		</basic>
    		<basic name="birthday">
    			<column name="birthday" nullable="true" />
    		</basic>
    		<basic name="header">
    			<column name="header" nullable="true" />
    		</basic>
    		<basic name="description">
    			<column name="description" nullable="true" />
    		</basic>
    		<basic name="registerTime">
    			<column name="registerTime" nullable="true" />
    		</basic>
    		<basic name="registerIp">
    			<column name="registerIp" nullable="true" />
    		</basic>
    		<basic name="code">
    			<column name="code" nullable="true" />
    		</basic>
    	</attributes>
    </entity>
</entity-mappings>